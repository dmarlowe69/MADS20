.PAGE 'LOLOADER20".OPT NOLIST;**********************************=$1201        ;START OF BASIC VIC 20 24K EX;*********************************; SET BSTUB TO ONE TO ALLOW FOR; INSERTION OF BASIC STUB CODE;*********************************BSTUB = 1;*********************************;;         LOLOADER20;      LOLOAD.V20 V072882;LOAD OBJECT FILE FROM ASSEMBLER20; (C) 1982 BY COMMODORE MACHINES;;  DOCUMENTED BY DENTON MARLOWE;*********************************;; BASIC FORMAT PROGRAM;; 10 SYS(4623);;*********************************.IFN BSTUB < .WORD BASIC    ;LINE LINK POINTER .BYTE $0A,$00  ;LINE NUMBER (10) .BYTE 158      ;SYS COMMAND TOKEN .BYTE '(4623)' ;CALL TO $080F .BYTE $00      ;END OF LINEBASIC .BYTE $00,$00  ;END OF BASIC>;*********************************;*=$120F        ;LOADER64 ORGIN;*********************************; EQUATES FOR TITLE IN MAIN BODY; SOURCE CODE FILE;; LOLOADER.ASM OR HILOADER.ASM;;.BYTE 'LOLOAD.V20 V072882';.BYTE 'HILOAD.V20 V072882';.BYTE TITLE1,TITLE2;.BYTE 'LOAD.V20 V072882';.BYTE $0D;;*********************************TITLE1=$4C ;'L'TITLE2=$4F ;'O';*********************************; SET FLAG TO ALLOW BASIC; AND KERNAL (ERROR) MESSAGES; $80 FOR BASIC; $40 FOR KERNAL;*********************************SETMSG=$FF90 LDA #$C0     ;ALLOW ALL MSG JSR SETMSG;*********************************; MAIN BODY OF CODE;*********************************.FIL LOADER.ASM;*********************************.END